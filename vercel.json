{
  "version": 2,
  "builds": [
    {
      "src": "api/tiktok.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/tiktok",
      "methods": ["GET"],
      "dest": "/api/tiktok.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}